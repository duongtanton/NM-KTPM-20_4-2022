                <form class="form-horizontal form-edit-enterprise" action="/admin/profiles?_method=PATCH" method="POST">
                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> Exterprice code
                        </label>
                        <div class="col-sm-9">
                            <input type="text" name="code" value="{{_user.enterprise.code}}" id="form-field-1-1"
                                readonly="" placeholder="Exterprice code" class="col-xs-9" />
                        </div>
                    </div>
                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> Name </label>
                        <div class="col-sm-9">
                            <input type="text" name="name" value="{{_user.enterprise.name}}" id="form-field-1"
                                placeholder="Name" class="col-xs-9" />
                        </div>
                    </div>
                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> Localtion</label>
                        <div class="col-sm-9">
                            <input type="text" name="localtion" value="{{_user.enterprise.localtion}}" id="form-field-1"
                                placeholder="Localtion" class="col-xs-9" />
                        </div>
                    </div>

                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> Email </label>
                        <div class="col-sm-9">
                            <input type="email" name="email" value="{{_user.enterprise.email}}" id="form-field-2"
                                placeholder="abc@gmail.com" class="col-xs-9" />
                        </div>
                    </div>

                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-field-2"> Phone </label>
                        <div class="col-sm-9">
                            <input type="text" name="phone" value="{{_user.enterprise.phone}}" id="form-field-2"
                                placeholder="Phone" class="col-xs-9" />
                        </div>
                    </div>

                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-field-2">Verified </label>
                        <div class="col-xs-3">
                            <label style="padding-top: 4px;">
                                <input checked="{{_user.enterprise.verified}}" name="verified"
                                    class="ace ace-switch ace-switch-5" type="checkbox" />
                                <span class="lbl"></span>
                            </label>
                        </div>
                    </div>

                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-input-readonly"> Joined</label>

                        <div class="col-sm-9">
                            <input readonly="" type="text" class="col-xs-9" id="form-input-readonly"
                                value="{{ formatDate _user.enterprise.createAt }}" />
                        </div>
                    </div>

                    <div class="space-4"></div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label no-padding-right" for="form-input-readonly"> Last updated
                        </label>

                        <div class="col-sm-9">
                            <input readonly="" type="text" class="col-xs-9" id="form-input-readonly"
                                value="{{ formatDate _user.enterprise.updateAt }}" />
                        </div>
                    </div>

                    <div class="space-4"></div>

                    <h4 class="header green clearfix">
                        About me
                    </h4>
                    <div class="wysiwyg-editor exterprise-description" id="editor1"></div>

                    <div class="space-4"></div>

                    <div class="clearfix form-actions">
                        <div class="col-md-offset-3 col-md-9">
