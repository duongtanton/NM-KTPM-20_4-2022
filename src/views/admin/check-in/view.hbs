<!-- inline scripts related to this page -->
<script>
    const checkOutList = {{checkOut.checkInId}};
    const div = document.getElementById("table-body");
    const nodeTR = div.getElementsByTagName("tr");    
    
    window.onload = function (){
        for(let i = 0; i < nodeTR.length; i++){
            let nodeTD = nodeTR[i].getElementsByTagName("td");
            let checkInId = nodeTD[1].textContent;
            console.log(checkInId);
            if(checkOutList.includes(checkInId)){
                let checkOutStatus = nodeTD[7].textContent = "Checked-out";
            }
        }
    }

</script>